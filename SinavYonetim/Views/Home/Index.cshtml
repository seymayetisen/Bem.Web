@model List<SinavYonetim.Models.Person>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div>
        <ul>
            @foreach(var p in Model)
            {
                <li>@p.Isim @p.Surname @p.IdentityNumber</li>
            }
        </ul>
    </div>
</body>
</html>
